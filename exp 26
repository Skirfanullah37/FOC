#include <stdio.h>

int main() {
    int n, i;
    int num, max;

    printf("Enter the total number of integers: ");
    scanf("%d", &n);

    // Read the first number
    printf("Enter number 1: ");
    scanf("%d", &max);

    // Compare the remaining numbers
    for (i = 2; i <= n; i++) {
        printf("Enter number %d: ", i);
        scanf("%d", &num);

        if (num > max) {
            max = num;
        }
    }

    printf("The biggest number is: %d\n", max);

    return 0;
}
