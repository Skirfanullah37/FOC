#include <stdio.h>

int calculateOddSeriesSum(int n) {
    int sum = 0, i;
    for (i = 1; i <= n; i += 2) {
        sum += i;
    }
    return sum;
}

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    int oddSeriesSum = calculateOddSeriesSum(n);

    printf("Sum of the odd number series from 1 to %d = %d\n", n, oddSeriesSum);

    return 0;
}
