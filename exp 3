#include <stdio.h>

void generateOddSeries(int n) {
    int i;
    for (i = 1; i <= n; i += 2) {
        printf("%d ", i);
    }
    printf("\n");
}

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    printf("Odd number series from 1 to %d: ", n);
    generateOddSeries(n);

    return 0;
}
