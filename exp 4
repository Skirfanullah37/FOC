#include <stdio.h>

void generateFibonacciSeries(int n) {
    int first = 0, second = 1, next, i;
  
    printf("Fibonacci series up to %d: ", n);
  
    for (i = 0; i <= n; i++) {
        printf("%d ", first);
        next = first + second;
        first = second;
        second = next;
    }
  
    printf("\n");
}

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    generateFibonacciSeries(n);

    return 0;
}
